  "scripts": {
    "codegen:generate:once": "graphql-codegen",
    "codegen:generate:watch": "graphql-codegen --watch",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio --experimental",
    "prisma:migrate:save": "prisma migrate save --experimental",
    "prisma:migrate:up": "prisma migrate up --experimental",
    "nexus:reflection": "nexus dev --reflection --entrypoint pages/api/graphql.ts",
    "nexus:build": "nexus build --entrypoint pages/api/graphql.ts",
    "next:build": "next build",
    "next:watch": "next",
    "typecheck": "tsc --noEmit",
    "generate": "npm-run-all -p nexus:reflection codegen:generate:once",
    "dev": "npm-run-all -p nexus:reflection codegen:generate:watch next:watch",
    "build": "npm-run-all -s nexus:build next:build",
    "start": "next start"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.15.4",
    "@graphql-codegen/fragment-matcher": "^1.15.4",
    "@graphql-codegen/near-operation-file-preset": "^1.15.4",
    "@graphql-codegen/typescript": "^1.15.4",
    "@graphql-codegen/typescript-operations": "^1.15.4",
    "@graphql-codegen/typescript-react-apollo": "^1.15.4",
    "@prisma/cli": "2.1.3",
    "@types/node": "^14.0.14",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/rebass": "^4.0.6",
    "@types/rebass__forms": "^4.0.2",
    "apollo": "^2.28.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "@apollo/client": "^3.0.0-rc.9",
    "@nexus/schema": "^0.14.0",
    "@prisma/client": "2.1.3",
    "@rebass/forms": "^4.0.6",
    "emotion-theming": "^10.0.27",
    "immer": "^7.0.5",
    "next": "^9.4.4",
    "next-auth": "^2.1.0",
    "nexus": "^0.24.2",
    "nexus-plugin-prisma": "^0.10.0",
    "normalize.css": "^8.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-linkify": "^1.0.0-alpha",
    "rebass": "^4.0.7"
  }
