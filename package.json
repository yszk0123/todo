  "scripts": {
    "codegen:generate:once": "graphql-codegen",
    "codegen:generate:watch": "graphql-codegen --watch",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio --experimental",
    "prisma:migrate:save": "prisma migrate save --experimental",
    "prisma:migrate:up": "prisma migrate up --experimental",
    "nexus:reflection": "nexus dev --reflection --entrypoint pages/api/graphql.ts",
    "nexus:dev": "nexus dev --entrypoint pages/api/graphql.ts",
    "nexus:build": "nexus build --entrypoint pages/api/graphql.ts",
    "next:build": "next build",
    "next:watch": "next",
    "lint": "eslint pages client server --ext .ts,.tsx",
    "lint:fix": "eslint pages client server --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "generate": "npm-run-all -p nexus:reflection codegen:generate:once",
    "dev": "npm-run-all -p nexus:dev codegen:generate:watch next:watch",
    "build": "npm-run-all -s prisma:generate nexus:build next:build",
    "start": "next start"
  },
